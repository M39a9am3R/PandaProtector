{
	"compilerOptions": {
		"incremental": true,
		"target": "ES6",
		"module": "CommonJS",
		"rootDir": "src/",
		"outDir": "dist/",
		"downlevelIteration": true,
		"resolveJsonModule": true,
		"strict": true,
		"typeRoots": ["node_modules/@types"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"importsNotUsedAsValues": "error"
	}
}
